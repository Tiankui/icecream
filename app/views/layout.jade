!!! 5
html
    head
        meta(http-equiv="Content-Type",content="text/html; charset=utf-8")
        - var title = '移动应用统计分析平台'
        title=title

        link(rel='stylesheet', type="text/css",href='/css/default.css')
        link(rel='stylesheet', type="text/css",href='/css/jquery.noty.css')
        link(rel='stylesheet', type="text/css",href='/css/noty_theme_default.css')
        link(rel='stylesheet', type="text/css",href='/css/buttons.css')
        link(rel='stylesheet', type="text/css",href='/js/fancybox/jquery.fancybox.css')        
        script(src='/js/jquery.min.js')
        script(src='/js/jquery.tablesorter.js')
        script(src='/js/jquery.noty.js')
        script(src='/js/fancybox/jquery.fancybox.pack.js')
        script(src='/js/md5.js')
        script(src='/js/common.js')
    body
        include ./header.jade
        div(class="main")
            !{body}           
        include ./footer.jade

    div(id="sheetBoxLeft",style="position:fixed;left:-400px;bottom:0px;background:#fff;height:100%;width:300px;border-right:3px solid #666;")

    div(id="sheetBoxRight",style="position:fixed;right:-400px;bottom:0px;background:#fff;height:100%;width:300px;border-left:3px solid #666;")

    div(id="sheetBoxTop",style="position:fixed;display:none;left:0;top:0px;background:#fff;width:100%;height:300px;border-right:3px solid #666;")

    div(id="sheetBoxBottom",style="position:fixed;display:none;left:0px;bottom:0px;background:#fff;width:100%;height:300px;border-right:3px solid #666;")
    